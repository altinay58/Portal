@model Portal.Models.Tema

@{
    ViewBag.Title = "Delete";
}

<hr />

<h3>Silmek İstediğinizden Emin misiniz ?</h3>
<div>
    <h4>Tema</h4>

    <dl class="dl-horizontal">
        <dt>
            Tema Adresi
        </dt>

        <dd>
            @Html.DisplayFor(model => model.temaAdresi)
        </dd>

        <dt>
            Firma Adı
        </dt>

        <dd>
            @Html.DisplayFor(model => model.temaFirmaAdi)
        </dd>

        <dt>
            Demo Adı

        <dd>
            @Html.DisplayFor(model => model.temaDemoAdi)
        </dd>

        <dt>
            Proje Tipi
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DomainKategori.DomainKategoriAdi)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Sil" class="btn btn-default" /> |
            @Html.ActionLink("Listeye Geri Dön", "List")
        </div>
    }
</div>
